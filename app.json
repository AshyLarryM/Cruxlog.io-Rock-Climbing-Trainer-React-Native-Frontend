{
  "expo": {
    "name": "CruxLog",
    "slug": "cruxlog",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/cruxlogIcon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/cruxlogIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "CruxLog requires access to your photo library to allow you to upload profile pictures for personalization and climb images to document your climbing sessions.",
        "NSCameraUsageDescription": "CruxLog uses the camera to capture photos of climbs for your climbing session data/history."
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.cruxlog"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.cruxlog"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/adaptive-icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
